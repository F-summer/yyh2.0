@import "/pages/template/online-push.wxss";
page {
  font-family: -apple-system,
                 BlinkMacSystemFont,
                 'Segoe UI',
                 Roboto,
                 Oxygen,
                 Ubuntu,
                 Cantarell,
                 'Open Sans',
                 'Helvetica Neue',
                 sans-serif;
}

view, text,scroll-view,input,navigator {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  line-height: 1;
  text-indent: 0rpx;
  padding-left:0rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
::-webkit-scrollbar {
     display: none;
}